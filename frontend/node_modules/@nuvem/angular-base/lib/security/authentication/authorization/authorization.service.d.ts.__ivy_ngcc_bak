import { Authorization } from './authorization';
import { Authentication } from '../authentication';
import { User } from '../user';
/**
 * Serviço de autorização
 * @class
 */
export declare class AuthorizationService extends Authorization {
    private authenticationService;
    /**
     * Método construtor para a injeção do serviço de configuração AuthConfig
     * @constructor
     * @param {AuthConfig} config
     */
    constructor(authenticationService: Authentication<User>);
    /**
     * Método responsável por identificar as regras de acesso
     * @public
     * @param {any} role
     * @returns boolean
     */
    hasRole(role: any): boolean;
}
