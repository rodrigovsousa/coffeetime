import { __decorate } from "tslib";
import { Pipe } from "@angular/core";
let CheckPipe = class CheckPipe {
    transform(value, ...args) {
        return value ? "Sim" : "NÃ£o";
    }
};
CheckPipe = __decorate([
    Pipe({
        name: "check"
    })
], CheckPipe);
export { CheckPipe };
