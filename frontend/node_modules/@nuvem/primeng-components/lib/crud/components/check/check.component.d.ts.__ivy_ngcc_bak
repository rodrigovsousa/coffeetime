import { Injector } from "@angular/core";
import { FormNotificationService } from "@nuvem/angular-base";
import { FieldComponent } from "../field.component";
import { ControlContainer } from '@angular/forms';
export declare class CheckComponent extends FieldComponent<string> {
    protected formNotificationService: FormNotificationService;
    private subscription;
    message: String;
    /**
     * Método construtor responsável por carregar o serviço de validação
     * @param formNotificationService
     */
    constructor(controlContainer: ControlContainer, injector: Injector, formNotificationService: FormNotificationService);
    /**
     * Método Inicial para subscrição no endpoint responsável por validar o formulário
     */
    ngOnInit(): void;
    /**
     * Método responsável por realizar a saida das subscrições do endpont de validação
     */
    ngOnDestroy(): void;
}
