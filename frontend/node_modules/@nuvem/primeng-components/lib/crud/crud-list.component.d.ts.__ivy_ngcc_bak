import { OnInit, EventEmitter, AfterContentInit, QueryList } from "@angular/core";
import { FormGroup } from "@angular/forms";
import { CrudService } from "./crud-service.service";
import { Router, ActivatedRoute } from "@angular/router";
import { Column } from '../datatable/p-column.directive';
import { DatatableComponent } from '../datatable/datatable.component';
import { DatatableClickEvent } from '../datatable/datatable-click-event';
import { ConfirmationService } from 'primeng/api';
export declare class CrudListComponent implements OnInit, AfterContentInit {
    crudService: CrudService<any, any>;
    private confirmationService;
    private route;
    private router;
    form: FormGroup;
    action: string;
    findLabel: string;
    service: CrudService<any, any>;
    url: string;
    disableSave: boolean;
    disableEdit: boolean;
    disableView: boolean;
    disableDelete: boolean;
    datatableExtraParams: any;
    find: EventEmitter<any>;
    datatable: DatatableComponent;
    columns: QueryList<Column>;
    result: any[];
    constructor(crudService: CrudService<any, any>, confirmationService: ConfirmationService, route: ActivatedRoute, router: Router);
    ngOnInit(): void;
    datatableClick(event: DatatableClickEvent): void;
    findAll(event: any): void;
    ngAfterContentInit(): void;
    ngAfterViewInit(): void;
}
