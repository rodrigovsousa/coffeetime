import { EventEmitter } from '@angular/core';
/**
 * Class DatatableButtonComponent
 * @class
 */
export declare class DatatableButtonComponent {
    /**
     * icon property
     * @type {string}
     */
    icon: string;
    /**
     * class property
     * @type {string}
     */
    class: string;
    /**
     * bTooltip property
     * @type {string}
     */
    bTooltip: string;
    /**
     * name property
     * @type {string}
     */
    name: string;
    /**
     * allowMultipleSelection property
     * @type {boolean}
     */
    allowMultipleSelection: boolean;
    /**
     * disabled property
     * @type boolean
     */
    disabled: boolean;
    /**
     * click property
     * @type {EventEmitter<any>}
     */
    click: EventEmitter<any>;
    /**
     * onClick method
     * @param {any} event
     * @return void
     */
    onClick(event: any): void;
}
