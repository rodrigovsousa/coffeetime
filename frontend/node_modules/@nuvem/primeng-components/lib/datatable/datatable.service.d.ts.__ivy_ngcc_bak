import { LazyLoadEvent } from 'primeng';
import { HttpClient } from '@angular/common/http';
/**
 * Class DatatableService
 * @class
 */
export declare class DatatableService {
    private http;
    /**
     * constructor method
     * @constructor
     * @param {HttpClient} http
     */
    constructor(http: HttpClient);
    /**
     * search method
     * @param {LazyLoadEvent} event
     * @param {String} url
     * @param {number} rows
     * @param {boolean} orderInSort
     * @param {any} query
     * @return Observable
     */
    search(event: LazyLoadEvent, url: string, rows: number, orderInSort: boolean, query?: any): import("rxjs").Observable<import("@angular/common/http").HttpResponse<Object>>;
}
