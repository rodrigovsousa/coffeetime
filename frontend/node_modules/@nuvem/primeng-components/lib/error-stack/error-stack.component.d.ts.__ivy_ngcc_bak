import { OnInit, OnDestroy, TemplateRef } from '@angular/core';
import { ErrorStackService, NamedErrorType } from '@nuvem/angular-base';
/**
 * Class ErrorStackComponent
 * @class
 */
export declare class ErrorStackComponent implements OnInit, OnDestroy {
    private errorStack;
    /**
     * title property
     * @type {string}
     */
    title: string;
    /**
     * template property
     * @type {TemplateRef}
     */
    template: TemplateRef<any>;
    /**
     * errors property
     * @type {NamedErrorType}
     */
    errors: NamedErrorType[];
    /**
     * errorSubscription property
     * @type {Subscription}
     */
    private errorSubscription;
    /**
     * constructor method
     * @constructor
     * @param {ErrorStackService} errorStack
     */
    constructor(errorStack: ErrorStackService);
    /**
     * ngOnInit method
     * @returns void
     */
    ngOnInit(): void;
    /**
     * ngOnDestroy method
     * @returns void
     */
    ngOnDestroy(): void;
}
