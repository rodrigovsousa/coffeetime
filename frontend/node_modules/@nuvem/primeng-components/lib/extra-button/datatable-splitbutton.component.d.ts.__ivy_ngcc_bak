/**
 * Class DatatableSplitbuttonComponent
 * @class
 */
export declare class DatatableSplitbuttonComponent implements OnInit {
    /**
     * items property
     * @type {any[]}
     */
    items: any[];
    /**
     * label property
     * @type {string}
     */
    label: string;
    /**
     * class property
     * @type {string}
     */
    class: string;
    /**
     * icon property
     * @type {string}
     */
    icon: string;
    /**
     * method propety
     * @type {any}
     */
    method: any;
    /**
     * ngOnInit method
     * @returns void
     */
    ngOnInit(): void;
}
