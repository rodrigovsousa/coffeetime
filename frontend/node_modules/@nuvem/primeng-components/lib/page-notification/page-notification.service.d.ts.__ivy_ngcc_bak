import { MessageService } from 'primeng';
import { NotificationProvider, ApplicationProblemType } from '@nuvem/angular-base';
/**
 * Class PageNotificationService
 * @class
 */
export declare class PageNotificationService extends NotificationProvider {
    private messageService;
    /**
     * successSeverity property
     * @type {string}
     */
    private readonly successSeverity;
    /**
     * infoSeverity property
     * @type {string}
     */
    private readonly infoSeverity;
    /**
     * warnSeverity property
     * @type {string}
     */
    private readonly warnSeverity;
    /**
     * errorSeverity property
     * @type {string}
     */
    private readonly errorSeverity;
    /**
     * createMsg property
     * @type {string}
     */
    private readonly createMsg;
    /**
     * updateMsg property
     * @type {string}
     */
    private readonly updateMsg;
    /**
     * deleteMsg property
     * @type {string}
     */
    private readonly deleteMsg;
    /**
     * constructor method
     * @constructor
     * @param {MessageService} messageService
     */
    constructor(messageService: MessageService);
    /**
     * addCreateMsg method
     * @param {string} title
     */
    addCreateMsg(title?: string): void;
    /**
     * addSuccessMessage method
     * @param {string} message
     * @param {string} title
     */
    addSuccessMessage(message: string, title?: string): void;
    /**
     * addMsg method
     * @param {string} severity
     * @param {string} msg
     * @param {string} title
     */
    private addMsg;
    /**
     * addUpdateMsg method
     * @param {string} title
     */
    addUpdateMsg(title?: string): void;
    /**
     * addDeleteMsg method
     * @param {string} title
     */
    addDeleteMsg(title?: string): void;
    /**
     * addInfoMessage method
     * @param {string} message
     * @param {string} title
     */
    addInfoMessage(message: string, title?: string): void;
    /**
     * addWarnMessage method
     * @param {string} message
     * @param {string} title
     */
    addWarnMessage(message: string, title?: string): void;
    /**
     * addErrorMessage method
     * @param {string} message
     * @param {string} title
     */
    addErrorMessage(message: string, title?: string): void;
    /**
     * addErrorProblem method
     * @param {ApplicationProblemType} problem
     */
    addErrorProblem(problem: ApplicationProblemType): void;
}
